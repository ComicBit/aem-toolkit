{
  "manifest_version": 3,
  "name": "AEM Toolkit",
  "version": "0.1",
  "description": "Adds few functions to navigate AEM quickly.",
  "permissions": ["activeTab", "scripting", "contextMenus"],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
          "16": "icons/icon16.png",
          "32": "icons/icon32.png",
          "48": "icons/icon48.png",
          "128": "icons/icon128.png"
  }, 
  "content_scripts": [
    {
      "matches": ["http://localhost:4502/*", "http://10.29.24.20:4502/*"],
      "js": ["content.js"],
      "css": ["style.css"]
    }
  ]
}